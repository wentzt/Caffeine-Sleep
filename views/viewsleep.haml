%html
  %head
    %link{:rel => "stylesheet", :type => "text/css", :href => "stylesheet.css"}
    %title Caffeine-Sleep: Sleep
  %body
    .header-nav
      = renderPartial "nav_after_login"
    #content-frame
      #content
        - if @sleepEntries.empty?
          %h1 No Sleep Entries Found
        #sleep
          - @sleepEntries.each do |sleep|
            %p Length: 
            %span
              = sleep.length
